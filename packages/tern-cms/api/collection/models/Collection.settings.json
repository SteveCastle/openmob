{
  "connection": "default",
  "collectionName": "collections",
  "info": {
    "name": "collection",
    "description": "A collection of product items."
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "comment": ""
  },
  "attributes": {
    "products": {
      "collection": "product",
      "dominant": true,
      "via": "collections"
    },
    "title": {
      "default": "",
      "unique": true,
      "type": "string",
      "required": true
    },
    "promotions": {
      "collection": "promotion",
      "via": "collections"
    }
  }
}